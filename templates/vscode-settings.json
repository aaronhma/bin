{
    "code-runner.saveFileBeforeRun": true,
    "code-runner.preserveFocus": false,
    "code-runner.ignoreSelection": true,
    "code-runner.runInTerminal": true,
    "debug.toolBarLocation": "docked",
    "editor.codeActionsOnSave": {
        "source.organizeImports": "never"
    },
    "editor.codeLens": true,
    "editor.emptySelectionClipboard": false,
    "editor.fontSize": 18,
    "editor.insertSpaces": true,
    "editor.lineHeight": 35,
    "editor.minimap.renderCharacters": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.renderLineHighlight": "all",
    "editor.roundedSelection": true,
    "editor.rulers": [
        100
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.snippetSuggestions": "top",
    "editor.suggestFontSize": 16,
    "editor.suggestLineHeight": 28,
    "editor.tabSize": 4,
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.wrappingIndent": "indent",
    "emmet.triggerExpansionOnTab": true,
    "explorer.openEditors.visible": 1,
    "explorer.decorations.badges": false,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 2000,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "scm.diffDecorationsGutterWidth": 2,
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/build": true,
        "**/dist": true
    },
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "workbench.colorCustomizations": {
        "statusBar.background": "#212121",
        "statusBar.noFolderBackground": "#212121",
        "statusBar.debuggingBackground": "#212121",
        "editorUnnecessaryCode.border": "#383737",
        "[Material Theme Darker]": {}
    },
    "workbench.commandPalette.history": 10,
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.showTabs": "single",
    "workbench.fontAliasing": "antialiased",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.list.multiSelectModifier": "alt",
    "workbench.startupEditor": "none",
    "window.closeWhenEmpty": true,
    "breadcrumbs.enabled": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.suggestSelection": "first",
    "workbench.statusBar.visible": true,
    "editor.wordWrap": "on",
    "git.suggestSmartCommit": false,
    "python.languageServer": "Default",
    "window.restoreWindows": "none",
    "editor.accessibilitySupport": "off",
    "[markdown]": {
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false,
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        },
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "window.zoomLevel": 1,
    "[shellscript]": {
        "files.eol": "\n"
    },
    "[javascript]": {
        "editor.maxTokenizationLineLength": 2500,
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "telemetry.telemetryLevel": "off",
    "gitlens.telemetry.enabled": false,
    "code-runner.enableAppInsights": false,
    "code-runner.executorMap": {
        "javascript": "node",
        "cpp": "cd $dir && g++ -std=c++17 -Wall -Wshadow -Wextra -Wpedantic -O2 -ld_classic $fileName && ./a.out",
        "python": "python -u",
        "go": "go run"
    },
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "editor.formatOnSave": true,
    "editor.hideCursorInOverviewRuler": true,
    "editor.minimap.enabled": false,
    "editor.glyphMargin": true,
    "editor.folding": true,
    "workbench.colorTheme": "Material Theme Darker",
    "editor.cursorBlinking": "phase",
    "editor.overviewRulerBorder": false
}
